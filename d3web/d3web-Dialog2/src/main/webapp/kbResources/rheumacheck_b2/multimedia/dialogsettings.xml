<?xml version="1.0" encoding="UTF-8"?>
<DialogSettings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="dialogsettings.xsd">
	<Dialog>
		<leftPanelSizePercent value="5.0" />
		<rightPanelSizePercent value="5.0" />
		<allowRightPanel value="false" />
		<showProgressBar value="true" />
		<showRightPanelToggleButtons value="false" />
		<saveCaseThreadMaxIdleTime value="0" />
		<!--
			time in milliseconds when the savecasethread will be executed. '0'
			for no saveCaseThread
		-->
		<sessionMaxInactiveInterval value="7200" /> <!-- time in seconds when the HTTP session will expire -->
		<showManagementButton value="false" />
		<showDialogButton value="false" />
		<timeZone value="Europe/Berlin" />
		<showCountryFlags value="false" />
		<showPageHeader value="false" />
		<showPageFooter value="false" />
	</Dialog>
	<QuestionPage>
		<autoMoveToResultpage value="true" /> <!-- move to result page if all containers are answered by the user -->
		<showQuestionPageAnswerButton value="true" />
		<showQuestionPageResultButton value="false" />
		<showQuestionPageUnknownButton value="false" />
		<questionMinAnsWrap value="6" />
		<questionDateDefaultDatesection value="full" />
		<!--
			possible options: 'date', 'time', 'full'
		-->
		<dialogMode value="MQ" />
		<!--
			possible options: 'MQ', 'OQ'
		-->
		<mCConstraintsAutoGrayOut value="true" />
		<showQASetTreeTab value="false" />
		<showDiagnosesTreeTab value="false" />
	</QuestionPage>
	<Diagnoses>
		<showHeuristicDiagnoses value="true" />
		<showHeuristicEstablishedDiagnoses
			value="true" />
		<showHeuristicSuggestedDiagnoses
			value="true" />
		<showHeuristicExcludedDiagnoses value="true" />
	</Diagnoses>
	<Explanation>
		<showDiagExplanation value="true" />
		<showDiagReason value="true" />
		<showDiagConcreteDerivation value="true" />
		<showDiagDerivation value="true" />
	</Explanation>
	<MMInfo>
		<showMMInfo value="true" />
		<maxCharLengthInMMInfoPopup value="50" />
	</MMInfo>
	<CompareCase>
		<showCompareCase value="true" />
	</CompareCase>
	<SCM>
		<showSCM value="true" />
		<displayMinPercentage value="0.15" />
		<digitcount value="2" />
	</SCM>
	<ProcessedQContainers>
		<showProcessedQContainers value="true" />
		<showAll value="false" />
		<maxInput value="8" />
		<showUnknown value="false" />
		<qTextMode value="text" />
		<!--
			possible options: 'prompt', 'text'
		-->
		<showQContainerNames value="true" />
		<showUnknownIcon value="true" />
		<showQContainerNamesIcon value="true" />
	</ProcessedQContainers>
	<Frequentness>
		<showFrequentness value="true" />
	</Frequentness>
</DialogSettings>