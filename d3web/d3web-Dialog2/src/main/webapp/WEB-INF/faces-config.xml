<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
	version="1.2">
	<component>
		<component-type>de.d3web.dialog2.QuestionPage</component-type>
		<component-class>de.d3web.dialog2.component.html.UIQuestionPage</component-class>
	</component>
	<component>
		<component-type>de.d3web.dialog2.CompareCasePage</component-type>
		<component-class>de.d3web.dialog2.component.html.UICompareCasePage</component-class>
	</component>
	<component>
		<component-type>de.d3web.dialog2.FrequentnessTable</component-type>
		<component-class>de.d3web.dialog2.component.html.UIFrequentnessTable</component-class>
	</component>
	<component>
		<component-type>de.d3web.dialog2.KBLoad</component-type>
		<component-class>de.d3web.dialog2.component.html.UIKBLoad</component-class>
	</component>
	<component>
		<component-type>de.d3web.dialog2.HeuristicDiagnosesBox</component-type>
		<component-class>de.d3web.dialog2.component.html.UIHeuristicDiagnosesBox</component-class>
	</component>
	<component>
		<component-type>de.d3web.dialog2.CompareCaseBox</component-type>
		<component-class>de.d3web.dialog2.component.html.UICompareCaseBox</component-class>
	</component>
	<component>
		<component-type>de.d3web.dialog2.AnswerFrequentnessBox</component-type>
		<component-class>de.d3web.dialog2.component.html.UIAnswerFrequentnessBox</component-class>
	</component>
	<component>
		<component-type>de.d3web.dialog2.SCMBox</component-type>
		<component-class>de.d3web.dialog2.component.html.UISCMBox</component-class>
	</component>
	<component>
		<component-type>de.d3web.dialog2.SCMPage</component-type>
		<component-class>de.d3web.dialog2.component.html.UISCMPage</component-class>
	</component>
	<component>
		<component-type>de.d3web.dialog2.DiagWithInfo</component-type>
		<component-class>de.d3web.dialog2.component.html.UIDiagWithInfo</component-class>
	</component>
	<component>
		<component-type>de.d3web.dialog2.OutputTextWithoutId</component-type>
		<component-class>de.d3web.dialog2.component.html.UIOutputTextWithoutId</component-class>
	</component>
	<component>
		<component-type>de.d3web.dialog2.QContainerName</component-type>
		<component-class>de.d3web.dialog2.component.html.UIQContainerName</component-class>
	</component>
	<component>
		<component-type>de.d3web.dialog2.ProcessedQContainersBox</component-type>
		<component-class>de.d3web.dialog2.component.html.UIProcessedQContainersBox</component-class>
	</component>
	<component>
		<component-type>de.d3web.dialog2.Explanation</component-type>
		<component-class>de.d3web.dialog2.component.html.UIExplanation</component-class>
	</component>
	<component>
		<component-type>de.d3web.dialog2.CorrectionPage</component-type>
		<component-class>de.d3web.dialog2.component.html.UICorrectionPage</component-class>
	</component>
	<component>
		<component-type>de.d3web.dialog2.MMInfoPage</component-type>
		<component-class>de.d3web.dialog2.component.html.UIMMInfoPage</component-class>
	</component>
	<managed-bean>
		<managed-bean-name>webDialog</managed-bean-name>
		<managed-bean-class>de.d3web.dialog2.WebDialog</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>questionPage</managed-bean-name>
		<managed-bean-class>de.d3web.dialog2.QuestionPageBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>qaSetTree</managed-bean-name>
		<managed-bean-class>de.d3web.dialog2.QASetTreeBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>diagnosesTree</managed-bean-name>
		<managed-bean-class>de.d3web.dialog2.DiagnosesTreeBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pageDisplay</managed-bean-name>
		<managed-bean-class>de.d3web.dialog2.controller.PageDisplayController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dialogSettings</managed-bean-name>
		<managed-bean-class>de.d3web.dialog2.basics.settings.DialogSettings</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dialogLayout</managed-bean-name>
		<managed-bean-class>de.d3web.dialog2.basics.layout.DialogLayout</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>imageMapBean</managed-bean-name>
		<managed-bean-class>de.d3web.dialog2.imagemap.ImageMapBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>localeChanger</managed-bean-name>
		<managed-bean-class>de.d3web.dialog2.util.ChangeLocaleBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>compareCase</managed-bean-name>
		<managed-bean-class>de.d3web.dialog2.controller.CompareCaseController</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>kbUploadBean</managed-bean-name>
		<managed-bean-class>de.d3web.dialog2.controller.KBUploadController</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>resultPageBean</managed-bean-name>
		<managed-bean-class>de.d3web.dialog2.controller.ResultPageController</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>dia</property-name>
			<property-class>de.d3web.dialog2.WebDialog</property-class>
			<value>#{webDialog}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>processedQContainersBean</managed-bean-name>
		<managed-bean-class>de.d3web.dialog2.controller.ProcessedQContainersController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>kbLoadBean</managed-bean-name>
		<managed-bean-class>de.d3web.dialog2.controller.KBLoadController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>saveCaseBean</managed-bean-name>
		<managed-bean-class>de.d3web.dialog2.controller.SaveCaseController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>loadCaseBean</managed-bean-name>
		<managed-bean-class>de.d3web.dialog2.controller.LoadCaseController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>userBean</managed-bean-name>
		<managed-bean-class>de.d3web.dialog2.basics.usermanaging.UserBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>answerFrequentnessBean</managed-bean-name>
		<managed-bean-class>de.d3web.dialog2.frequentness.D3webAnswerFrequentnessImpl</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<navigation-case>
			<from-outcome>result</from-outcome>
			<to-view-id>/result.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>exppopup</from-outcome>
			<to-view-id>/explanation.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dialog</from-outcome>
			<to-view-id>/dialog.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<render-kit>
		<renderer>
			<component-family>javax.faces.Input</component-family>
			<renderer-type>de.d3web.dialog2.QuestionPage</renderer-type>
			<renderer-class>de.d3web.dialog2.render.QuestionPageRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Input</component-family>
			<renderer-type>de.d3web.dialog2.FrequentnessTable</renderer-type>
			<renderer-class>de.d3web.dialog2.render.FrequentnessTableRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Input</component-family>
			<renderer-type>de.d3web.dialog2.CompareCasePage</renderer-type>
			<renderer-class>de.d3web.dialog2.render.CompareCasePageRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Input</component-family>
			<renderer-type>de.d3web.dialog2.KBLoad</renderer-type>
			<renderer-class>de.d3web.dialog2.render.KBLoadRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Input</component-family>
			<renderer-type>de.d3web.dialog2.ProcessedQContainersBox</renderer-type>
			<renderer-class>de.d3web.dialog2.render.ProcessedQContainersBoxRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Output</component-family>
			<renderer-type>de.d3web.dialog2.HeuristicDiagnosesBox</renderer-type>
			<renderer-class>de.d3web.dialog2.render.HeuristicDiagnosesBoxRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Output</component-family>
			<renderer-type>de.d3web.dialog2.SCMBox</renderer-type>
			<renderer-class>de.d3web.dialog2.render.SCMBoxRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Output</component-family>
			<renderer-type>de.d3web.dialog2.SCMPage</renderer-type>
			<renderer-class>de.d3web.dialog2.render.SCMPageRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Output</component-family>
			<renderer-type>de.d3web.dialog2.Explanation</renderer-type>
			<renderer-class>de.d3web.dialog2.render.ExplanationRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Input</component-family>
			<renderer-type>de.d3web.dialog2.CorrectionPage</renderer-type>
			<renderer-class>de.d3web.dialog2.render.CorrectionPageRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Output</component-family>
			<renderer-type>de.d3web.dialog2.MMInfoPage</renderer-type>
			<renderer-class>de.d3web.dialog2.render.MMInfoPageRenderer</renderer-class>
		</renderer>
	</render-kit>
	<application>
		<locale-config>
			<default-locale>de</default-locale>
			<supported-locale>de</supported-locale>
			<supported-locale>en</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>de.d3web.dialog2.messages</base-name>
			<var>msgs</var>
		</resource-bundle>
		<message-bundle>de.d3web.dialog2.messages</message-bundle>
	</application>
</faces-config>