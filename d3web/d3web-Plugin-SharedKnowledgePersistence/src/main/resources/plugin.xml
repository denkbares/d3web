<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="knowledgePersistenceShared" version="0.0.1">
	
	<requires>
		<import plugin-id="KnowledgePersistenceExtensionPoints" />
	</requires>
	
	<runtime>
		<library id="de.d3web" path="." type="code">
			<export prefix="*" />
		</library>
	</runtime>
	
	<extension plugin-id="KnowledgePersistenceExtensionPoints" point-id="KnowledgeReader"
		id="SharedPersistenceHandler">
		<parameter id="class"
			value="de.d3web.shared.io.SharedPersistenceHandler" />
		<parameter id="name" value="SharedPersistenceHandler" />
		<parameter id="description" value="Loads shared knowledge from XML" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
		<parameter id="filename" value="kb/shared.xml" />
	</extension>
</plugin>